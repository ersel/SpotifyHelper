{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
  {
    "matches": ["https://play.spotify.com/*"],
    "js": ["scripts/contents.js"]
  }
],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}
